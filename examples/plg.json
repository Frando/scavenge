{
  "origin": "http://www.progressivelibrariansguild.org/PL_Jnl/jnl_contents.shtml",
  "find": "div.content p:not([class]) a",
  "next": {
    "follow": "@href",
    "find": "div.content",
    "variables": {
      "issue": "p.pagename",
      "edition": "p.subpagename",
      "title_raw": ["a !> p:not([class])"],
      "pdf": ["p:not([class]) a@href"]
    }
  },
  "actions": {
    "translate": {
      "title_raw": [
      {
        "match": "\\n(?:[\\s\\S]*by\\s([\\s\\S]+), )?page (\\d+) \\[\\.pdf file\\]",
        "to": {
          "author": "$1",
          "page": "$2"
        }
      }, {
        "match": "^([\\s\\S]+)\\n",
        "to": {
          "title": "$1"
        }
      }]
    },
    "opf": {
      "directory": "./Progressive Librarians/${issue}/"
    }
  }
}
