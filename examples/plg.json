{
  "origin": "http://www.progressivelibrariansguild.org/PL_Jnl/jnl_contents.shtml",
  "find": "div.content p:not([class]) a",
  "next": {
    "follow": "@href",
    "find": "p",
    "variables": {
      "section": "a !> p !~ p strong:last",
      "title_raw": "a !> p",
      "pdf": "a@href"
    },
    "filter": {
      "pdf": ".pdf"
    }
  },
  "actions": {
    "translate": {
      "title_raw": [
      {
        "match": "\\n(?:[\\s\\S]*by\\s([\\s\\S]+), )?page (\\d+) \\[\\.pdf file\\]",
        "to": {
          "author": "$1",
          "page": "$2"
        }
      }, {
        "match": "^([\\s\\S]+)\\n",
        "to": {
          "title": "$1"
        }
      }]
    },
    "download": {
      "url": "pdf",
      "directory": "Progressive Librarians Guild/${author}/${title}/",
      "overwrite": false
    }
  }
}
