{
  "origin": "http://ro.uow.edu.au/ozsydney/",
  "find": "ul#gallery_items li:nth-child(1) .content_block",
  "variables": {
    "title": "h2 > a",
    "date": "#mag_pubdate",
    "cover": ".cover img@src",
    "info": ".cover@href"
  },
  "next": {
    "follow": ".cover@href",
    "find": "#alpha",
    "variables": {
      "pdf": "#alpha-pdf@href"
    }
  },
  "actions": {
    "jsonManifest": {
      "filepath": "OZ/manifest.json",
      "properties": {
        "title": "${title}", 
        "date": "${date}", 
        "source": "${info}",
        "path": "OZ/${title} (${date})/"
      }
    },
    "readability": {
      "url": "${info}",
      "filepath": "OZ/${title} (${date})/index.html"
    },
    "mock": [
      {
        "url": "${pdf}",
        "filepath": "OZ/${title} (${date})/${title}.pdf",
        "overwrite": false
      },
      {
        "url": "${cover}",
        "filepath": "OZ/${title} (${date})/cover.jpg",
        "overwrite": false
      }
    ]
  }
}
